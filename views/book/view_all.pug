extends ../layout

block content
    h3 Books
    table
        thead
            tr
                th Title
                th Author
                th publisher
                th genre
                th Page Number
                th Cover
                th
        tbody
            each book in books
                tr
                    td= book.title
                    td= book.author
                    td= book.publisher
                    td= book.genre
                    td= book.pg_number
                    td= book.image